{
  tenant: mytenant
  channel: mychannel
  version: "6.0"
  runtime: storm
  dag:
  [
    {
      type: third_party_input
      component: input_node
      class: org.thales.punch.storm.node.starter.kit.InputNode
      settings:
      {
        user_name: alice
        load_control: rate
        load_control.rate: 1
      }
      publish:
      [
        {
          stream: logs
          fields:
          [
            log
          ]
        }
      ]
    }
    {
      type: punchlet_node
      component: show
      settings: {
        punchlet_code: "{ print(root); }"
      }
      subscribe:
      [
        {
          component: input_node
          stream: logs
        }
      ]
      publish: []
    }
  ]
  settings:
  {
    additional_jars:
    [
      punch-storm-starter-kit-1.0-SNAPSHOT-jar-with-dependencies.jar
    ]
  }
}